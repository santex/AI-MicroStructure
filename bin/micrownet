#! /bin/bash
for i in $@; do
wn $i -hypon -hypen -synsv -synsn -synsn -hypon -hypen -synsv -synsn -grepn -grepa -grepv | sed 's/[[:space:]]*=> /\n/' | sed '/^$/d' | sed 's/ /_/'  | sed 's/,/, /' | sed 's/HAS INSTANCE//g' |
sed 's/Sense//g' |
sed 's/[[:space:]]*=> //' |
sed 's/^[[:digit:]] senses of.*.$//'|
egrep -v "([ ]|[0-9][A-Z])" |
grep -i $1
done;

